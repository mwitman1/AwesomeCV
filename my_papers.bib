isbn = {1530-6984},
issn = {1530-6984},
journal = {Nano Lett.},
keywords = {GIWAXS,RSOXS,Self-assembly,all-conjugated block copolymers,organic solar cells},
month = {jun},
number = {6},
pages = {2957--2963},
pmid = {23687903},
title = {{Conjugated Block Copolymer Photovoltaics with near 3{\%} Efficiency through Microphase Separation}},
