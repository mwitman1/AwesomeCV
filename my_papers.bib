@article{Ongari2017a,
abstract = {{\textcopyright} 2017 American Chemical Society. Pore volume is one of the main properties for the characterization of microporous crystals. It is experimentally measurable, and it can also be obtained from the refined unit cell by a number of computational techniques. In this work, we assess the accuracy and the discrepancies between the different computational methods which are commonly used for this purpose, i.e, geometric, helium, and probe center pore volumes, by studying a database of more than 5000 frameworks. We developed a new technique to fully characterize the internal void of a microporous material and to compute the probe-accessible and -occupiable pore volume. We show that, unlike the other definitions of pore volume, the occupiable pore volume can be directly related to the experimentally measured pore volumes from nitrogen isotherms.},
author = {Ongari, Daniele and Boyd, Peter G. and Barthel, Senja and Witman, Matthew and Haranczyk, Maciej and Smit, Berend},
doi = {10.1021/acs.langmuir.7b01682},
issn = {0743-7463},
journal = {Langmuir},
month = {dec},
number = {51},
pages = {14529--14538},
title = {{Accurate Characterization of the Pore Volume in Microporous Crystalline Materials}},
url = {http://pubs.acs.org/doi/10.1021/acs.langmuir.7b01682},
volume = {33},
year = {2017}
}
